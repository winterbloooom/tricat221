<?xml version="1.0" encoding="UTF-8"?>

<!--bag and pub-->
<launch>
	<node pkg="tricat221"  type="heading_calculator.py" name="headingCalculator"/>
	<node pkg="tricat221" type="hopping_tour.py" name="hopping" output="screen"/>
	<node pkg="tricat221"  type="gnss_converter_rere.py" name="gnssConverter"/>

	<node pkg="rosbag" type="play" name="rosbag_play" required="true" args="$(find tricat221)/etc/rosbag/0806_hopping.bag -r 1.5 --topics /imu/data /imu/mag /ublox_gps/fix"/>

	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find tricat221)/rviz/rviz_setting2.rviz" />
	
	<!--PARAMETER-->
	<rosparam command="load" file="$(find tricat221)/params/lidar_params.yaml"/>
	<rosparam command="load" file="$(find tricat221)/params/hopping_params.yaml"/>
	<rosparam command="load" file="$(find tricat221)/params/coordinates.yaml"/>
	
</launch>